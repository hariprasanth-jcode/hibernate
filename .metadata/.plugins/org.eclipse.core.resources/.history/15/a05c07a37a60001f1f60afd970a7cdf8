import javax.persistence.Persistence;

public class Driver2 {
public static void main(String[] args) {
	EntityManagerFactory emf=Persistence.createEntityManagerFactory("bi");
	EntityManager em = emf.createEntityManager();
	
	em.find(Car.class, 1);
}
}
