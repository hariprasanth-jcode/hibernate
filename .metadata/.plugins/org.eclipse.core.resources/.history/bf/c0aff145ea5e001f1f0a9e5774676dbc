public class A {
public static void main(String[] args) {
	
	ApplicationContext ac = new AnnotationConfigApplicationContext(EmployeeConfig.class);
	EmployeeDao dao = ac.getBean("dao", EmployeeDao.class);
	System.out.println(dao);
	

}
}
